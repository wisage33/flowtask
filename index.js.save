import express from "express"
const app = express()
const port = 5000

const tasks = [
  {id: 1, title: "Buy GPU", completed: false},
  {id: 2, title: "Buy CPU", completed: true}
];

app.get("/api/tasks", (req, res) => {
  res.json(tasks);
});

app.get("/", (req, res) => {
  res.forEach
})

app.listen(port, () => console.log("Server listen at http://localhost:5000"))
